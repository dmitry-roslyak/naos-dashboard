extends layout

block content
    h1=title
    table(class="table table-hover")
        thead
            tr
                th ID
                th Name
                th Created Date
        tbody
            each user in users
                tr
                    td #{user.id}
                    td #{user.name}
                    td #{user.created_at}
    //- p Welcome #{JSON.stringify(user)}
block pagination
    include paginator
    +paginator(total, `/users/`)